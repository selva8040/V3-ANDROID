<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    tools:context=".Rewardpoints">

    <include layout="@layout/monthly_header" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:visibility="visible">

        <TextView
            android:id="@+id/textView34"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="16dp"
            android:gravity="center"
            android:text="view our report in\nhttps://www.v3healthzone.com/"
            android:textColor="@color/black"
            android:textSize="24sp" />

        <LinearLayout
            android:id="@+id/buttonslin"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="24dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/frdt"
                android:layout_width="300dp"
                android:layout_height="45dp"
                android:layout_marginLeft="8dp"
                android:layout_weight="1.2"
                android:background="@drawable/carddesign"

                android:gravity="center|center_vertical"
                android:padding="8dp"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/todt"
                android:layout_width="300dp"
                android:layout_height="45dp"
                android:layout_marginLeft="8dp"
                android:layout_weight="1.2"
                android:background="@drawable/carddesign"

                android:drawableRight="@drawable/ic_baseline_calendar_today_24px"
                android:gravity="center|center_vertical"

                android:padding="8dp"
                android:text="Select date"
                android:textColor="@color/black"
                android:textSize="14sp" />

        </LinearLayout>

        <Button
            android:id="@+id/button3"
            android:layout_width="120dp"
            android:layout_height="56dp"
            android:layout_gravity="center"
            android:layout_marginTop="16dp"
            android:background="@drawable/blue_solid_bg"
            android:text="VIEW"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/wholelin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/prolin"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"

                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/textView29"
                    android:layout_width="300dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="fill"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1.2"
                    android:gravity="fill_horizontal"
                    android:paddingLeft="32dp"
                    android:paddingTop="10sp"
                    android:text="Total Programmes"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView30"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1.2"
                    android:gravity="fill"
                    android:text="TextView"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/viewlin"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"

                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/textViewview29"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1.2"
                    android:gravity="fill_horizontal"
                    android:paddingLeft="32dp"
                    android:paddingTop="10sp"
                    android:text="Viewed"
                    android:textColor="@color/nav_head"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textViewview30"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1.2"
                    android:gravity="fill"
                    android:text="TextView"
                    android:textColor="@color/nav_head"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/noviewlin"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"

                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/notextViewview29"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1.2"
                    android:gravity="fill_horizontal"
                    android:paddingLeft="32dp"
                    android:paddingTop="10sp"
                    android:text="Not Viewed"
                    android:textColor="@color/color8red"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/notextViewview30"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1.2"
                    android:gravity="fill"
                    android:text="TextView"
                    android:textColor="@color/color8red"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/feedviewlin"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"

                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/feedtextViewview29"
                    android:layout_width="300dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="fill"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1.2"
                    android:gravity="fill_horizontal"
                    android:paddingLeft="32dp"
                    android:paddingTop="10sp"
                    android:text="Feedback Present"
                    android:textColor="@color/sample1"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/feedtextViewview30"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1.2"
                    android:gravity="fill"
                    android:text="TextView"
                    android:textColor="@color/sample1"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/feedprviewlin"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"

                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/feedprtextViewview29"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1.2"
                    android:gravity="fill_horizontal"
                    android:paddingLeft="32dp"
                    android:paddingTop="10sp"
                    android:text="Feedback Null"
                    android:textColor="@color/blue"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/feedprtextViewview30"
                    android:layout_width="300dp"
                    android:layout_height="45dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1.2"
                    android:gravity="fill"
                    android:text="TextView"
                    android:textColor="@color/blue"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>